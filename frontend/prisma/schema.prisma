// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Transactions {
  txHash String @unique
  address String 
  amount String
  github String?
  message String?
  createdAt DateTime @default(now())
}
